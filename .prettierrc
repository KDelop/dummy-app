{
  "tabWidth": 2,
  "printWidth": 110,
  "singleQuote": true,
  "arrowParens": "avoid",
  "semi": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "jsxSingleQuote": false,
  "newline-before-return": true,
  "no-duplicate-variable": [true, "check-parameters"],
  "no-var-keyword": true
}
